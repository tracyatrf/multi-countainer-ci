{
  "AWSEBDockerRunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "hostname": "client",
      "image": "tracymeade/multi-client",
      "essential": "false",
      "memory": 128
    },
    {
      "name": "server",
      "hostname": "api",
      "image": "tracymeade/multi-server",
      "essential": "false",
      "memory": 128
    },
    {
      "name": "nginx",
      "hostname": "nginx",
      "image": "tracymeade/multi-nginx",
      "essential": "true",
      "memory": 128,
      "portMappings": {
        "hostPort": 80,
        "containerPort": 80
      },
      "links": ["client","server"]
    },
    {
      "name": "worker",
      "hostname": "worker",
      "image": "tracymeade/multi-worker",
      "essential": "false",
      "memory": 128
    }
  ]
}
